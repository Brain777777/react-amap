{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "esnext",
    "moduleResolution": "node",
    "lib": ["esnext", "dom"],
    "esModuleInterop": true,
    "declaration": true,
    "jsx": "preserve",
    "paths": {
      "@pansy/react-amap/*": ["./src/*"],
    }
  },
  "exclude": [
    "node_modules",
    "dist",
    "**/*.spec.ts",
    "**/*.test.ts",
    "lib",
    "es",
    "fixtures",
    "examples"
  ]
}
